  <servlet>
    <servlet-name>administracionCategorias</servlet-name>
    <servlet-class>Servlets.administracionCategorias</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>administracionUsuarios</servlet-name>
    <servlet-class>Servlets.administracionUsuarios</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>agregarAlCarro</servlet-name>
    <servlet-class>Servlets.agregarAlCarro</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>verCarro</servlet-name>
    <servlet-class>Servlets.verCarro</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>pagarPedido</servlet-name>
    <servlet-class>Servlets.pagarPedido</servlet-class>
  </servlet>
</web-app>